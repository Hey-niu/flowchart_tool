{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.ts", "src/**/*.tsx", "src/**/**/*.vue", "src/**/**/*.tsx"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "jsx": "preserve",
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
